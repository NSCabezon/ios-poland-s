    <!--____________________________________________________  FECHAS _________________________________________________-->
    <!--   Fecha 1 (del 1 al 9)  -->
    <rule>
        <id>F0001</id>
        <expression><![CDATA[(GDM>=1)&&(GDM<=9)]]></expression>
    </rule>
    <!--  Fecha 2 (del 15 al 20)  -->
    <rule>
        <id>F0002</id>
        <expression><![CDATA[(GDM>=15)&&(GDM<=20)]]></expression>
    </rule>
    <!--  Fecha 3 (del 21 al 27)  -->
    <rule>
        <id>F0003</id>
        <expression><![CDATA[(GDM>=21)&&(GDM<=27)]]></expression>
    </rule>
    <!--  Fecha 4 (del 24 al 28)  -->
    <rule>
        <id>F0004</id>
        <expression><![CDATA[(GDM>=24)&&(GDM<=28)]]></expression>
    </rule>
    <!--  Fecha 5 (del 24 al 30)  -->
    <rule>
        <id>F0005</id>
        <expression><![CDATA[(GDM>=24)&&(GDM<=30)]]></expression>
    </rule>
    <!--  Fecha 6 (del 1 al 30)  -->
    <rule>
        <id>F0006</id>
        <expression><![CDATA[(GDM>=1)&&(GDM<=30)]]></expression>
    </rule>
    <!--   Fecha 7 (del 1 al 7)  -->
    <rule>
        <id>F0007</id>
        <expression><![CDATA[(GDM>=1)&&(GDM<=7)]]></expression>
